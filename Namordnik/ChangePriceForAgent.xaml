<Window x:Class="Namordnik.ChangePriceForAgent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Namordnik"
        mc:Ignorable="d"
        Title="ChangePriceForAgent" Height="200" Width="300" ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBox Name="TextBlockNewPrice" Width="200" TextChanged="TextBlockNewPrice_TextChanged"/>
            <Button Click="SaveChanges_Click" Width="100">
                <TextBlock>Прибавить</TextBlock>
            </Button>
        </StackPanel>
    </Grid>
</Window>
